{

	"manifest_version": 2,

	"name": "FSSeries",
	"short_name": "FSSeries",
	"description": "FSSeries",
	"version": "0.1.0",



	"background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },

	"content_scripts": [{
		"matches": [ "http://fs.to/*" ],
		"js": [ "js/contentscript.js" ],
		"run_at": "document_end"
	}],

	"browser_action": {
    "default_icon": {
      "30": "images/icon30.png"
    },
    "default_title": "FsSeries",
    "default_popup": "popup.html"
  }

}
